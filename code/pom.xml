    <version>1.5.0</version>
    </dependency>
  </dependencies>
	<build>
	<plugins>
	<plugin>
	<groupId>org.apache.maven.plugins</groupId>
	<artifactId>maven-compiler-plugin</artifactId>
	<version>2.3.1</version>
	<configuration>
	<source>1.5</source>
	<target>1.5</target>
	</configuration>
	</plugin>
	</plugins>
	</build>
</project>
